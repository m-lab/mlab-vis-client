{
  "name": "mlab-vis-client",
  "description": "The web client for Measurement Lab visualizations",
  "author": "Bocoup <hello@bocoup.com> (http://github.com/bocoup)",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bocoup/mlab-vis-client"
  },
  "homepage": "https://github.com/bocoup/mlab-vis-client",
  "keywords": [
    "mlab",
    "internet",
    "visualizations"
  ],
  "main": "bin/server.js",
  "scripts": {
    "build": "./build.sh",
    "deploy": "./deploy.sh -m ${API_MODE}",
    "dev": "concurrently --kill-others \"npm run start-dev\" \"npm run webpack-watch\"",
    "lint": "eslint -c .eslintrc src",
    "postinstall": "npm run webpack-clean-cache && env-cmd .env npm run build",
    "start": "./run.sh",
    "start-dev": "./run.sh -m development",
    "start-staging": "./run.sh -m staging",
    "start-production": "./run.sh -m production",
    "test": "karma start",
    "webpack-dll": "webpack --colors --progress --config webpack/dll.config.js && npm run webpack-clean-cache",
    "webpack-dashboard": "WEBPACK_DASHBOARD=true node webpack/webpack-dev-server.js",
    "webpack-watch": "better-npm-run webpack-watch",
    "webpack-clean-cache": "rm -rf webpack/webpack-records.json webpack/cache"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "webpack-watch": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "webpack-dashboard": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "WEBPACK_DASHBOARD": true
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
